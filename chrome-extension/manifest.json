{
	"name": "etc hosts (dns override)",
	"version": "0.0.1",
	"manifest_version": 2,
	"description": "指定ドメインの名前解決をoverride",
	"permissions": [
		"webRequest",
		"webRequestBlocking",
		"*://*.example.com/*",
		"*://127.0.0.1/*"
	],
	"background": {
		"persistent": true,
		"scripts": [
			"background.js"
		]
	}
}
